{
    "files.exclude": {
        ".dart_tool": true,
        ".idea": true,
        "**/*.freezed.dart": true,
        "**/*.g.dart": true,
        "**/*.h.dart": true, 
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "[dart]": {
        "editor.defaultFormatter": "Dart-Code.dart-code",
        "editor.formatOnSave": true
    },
}